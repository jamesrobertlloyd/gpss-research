keywords = {Alcohol-Related Disorders,Alcohol-Related Disorders: mortality,Algorithms,Computer Simulation,Disease,Epidemiologic Studies,Finland,Finland: epidemiology,Humans,Likelihood Functions,Markov Chains,Models,Monte Carlo Method,Normal Distribution,Poisson Distribution,Risk,Statistical,Stochastic Processes},
